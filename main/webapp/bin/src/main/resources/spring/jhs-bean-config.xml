<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="apiUrl" class="com.kcb.jhs.server.WmlpApiUrl" abstract="true">
		<property name="basePath" value="http://gw.lp.alibaba.com/openapi/" />
		<property name="protocol" value="xml2" />
		<property name="apiNameSpace" value="lp.alibaba.logistics.sandbox" />
		<property name="apiVersion" value="1" />
	</bean>
</beans>